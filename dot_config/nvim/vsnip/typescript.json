{
  "import": {
    "prefix": ["import"],
    "body": "import ${2:module} from \"$1\";"
  },
  "denops": {
    "prefix": ["denops"],
    "body": [
      "export function main(denops: Denops): Promise<void> {",
      "  denops.dispatcher = {",
      "    ${1:func}(${2:params}): Promise<void> {",
      "      $3",
      "    }",
      "  }",
      "};"
    ]
  }
}
