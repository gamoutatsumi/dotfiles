" setting
filetype plugin indent on
set number
set relativenumber
set autoindent
set tabstop=2
set shiftwidth=2
set expandtab
set splitright
set clipboard=unnamedplus
set hls
set foldmethod=marker
set mouse=a
set whichwrap=b,s,h,l,<,>,[,]
let g:loaded_fzf = 1
let g:python_host_prog = '/usr/bin/python2'
let g:python3_host_prog = '/usr/bin/python3'
colorscheme habamax
nnoremap x "_x
