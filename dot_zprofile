ARCH=$(uname -m)
if [[ -e /opt/homebrew/bin/brew ]] && [[ -e /usr/local/bin/brew ]]; then
  echo "Current Architecture: ${ARCH}"
  if [[ $ARCH == arm64 ]]; then
    eval $(/opt/homebrew/bin/brew shellenv)
  else
    eval $(/usr/local/bin/brew shellenv)
  fi
fi
[[ -e /home/linuxbrew/.linuxbrew/bin/brew ]] && eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)"

export DEIN_GITHUB_TOKEN=$(awk ' /oauth_token/ { print $2 }' ~/.config/gh/hosts.yml)
export HOMEBREW_GITHUB_API_TOKEN=$(awk ' /oauth_token/ { print $2 }' ~/.config/gh/hosts.yml)

# Added by Toolbox App
export PATH="$HOME/.ghcup/bin:$PATH:$HOME/.local/share/JetBrains/Toolbox/scripts"

